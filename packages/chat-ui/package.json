		"vite-plugin-lib-inject-css": "^2.1.1",
		"type-fest": "^4.26.1",
		"@vitejs/plugin-react": "^4.3.1",
		"vite": "^5.4.7",
		"vitest": "^2.1.1",
		"reshaped": "^3.2.0-canary.5",
